{
    "name": "withinboredom/distance",
    "description": "typesafe distance",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Withinboredom\\": "src/"
        },
        "files": [
            "src/Distance/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Rob Landers",
            "email": "landers.robert@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^v3.8.2",
        "laravel/pint": "^1.16.2"
    },
    "suggest": {
        "crell/serde": "for serialization"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
